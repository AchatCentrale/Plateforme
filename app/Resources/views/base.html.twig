<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <title>{% block title %}ACHAT-CENTRALE CRM{% endblock %}</title>
    <script src="//code.jquery.com/jquery-2.2.4.min.js"></script>


    {% block stylesheets %}


        <!-- Latest compiled and minified CSS -->
        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">



        <!-- Optional theme -->
        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css">



        <!-- Font Awesome -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.css" rel="stylesheet">
        <!-- Semantic ui -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.10/semantic.css" rel="stylesheet">
        <!-- sweet alert-->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css" rel="stylesheet">
        <!-- jquery ui-->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" rel="stylesheet">
        <!-- datatables bootstrap-->
        <link href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css" rel="stylesheet">

        <!-- DropZone css-->
        <link href="{{ asset('css/dropzone.css') }}" rel="stylesheet">
        <!-- Datatables css-->
        <link href="https://cdn.datatables.net/1.10.15/css/jquery.dataTables.min.css" rel="stylesheet">


        <!--Google font -->
        <link href="https://fonts.googleapis.com/css?family=Titillium+Web" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">


    {% endblock stylesheets %}

    <!-- Custom Theme Style -->
    <link href="{{ asset('css/custom.min.css') }}" rel="stylesheet">
    <link href="{{ asset('css/style.css') }}" rel="stylesheet">

</head>

<style>

    .ac-logo-icon {
        background-image: url({{ asset('assets/images/icon/logo-ac-icone.png') }});
    }

</style>

<body class="nav-md">
<div class="container body">
    <div class="main_container">


        {% include 'SiteBundle:Layout:Sidebar.html.twig' %}
        {% include 'SiteBundle:Layout:topnav.html.twig' %}


        <!-- page content -->
        <div class="right_col" role="main">
            <div class="container-content-page">
                {% block top_content_header %}
                    {#<div class="page-title">
                        <div class="title_left">
                            <h3>{% block page_title %}</h3>Page Title{% endblock page_title %}
                        </div>


                    </div>#}
                {% endblock top_content_header %}

                <div class="clearfix"></div>


                {% block body %}

                    {% set randomPassword = [] %}
                    {% for i in 1..10 %}
                        {% set alpha = 'abcdefghijklmnopqrstuvwxyz' %}
                        {% set alphaUpper = alpha|upper %}
                        {% set numbers = '0123456789' %}
                        {% set randomCharacter = random(alpha ~ alphaUpper ~ numbers ~ '-_') %}
                        {% set randomPassword = randomPassword|merge([randomCharacter]) %}
                    {% endfor %}
                    {% set randomPassword = randomPassword|join %}







                {% endblock %}


            </div>
        </div>
        <!-- /page content -->


        {% include 'SiteBundle:Layout:footer.html.twig' %}


    </div>
</div>

<!-- semantic ui -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.10/semantic.min.js"></script>
<!-- jQuery ui -->
<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>






{#boostrap validator#}
<script src="https://cdnjs.cloudflare.com/ajax/libs/1000hz-bootstrap-validator/0.11.9/validator.js"></script>
<!-- Bootstrap -->

<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<!-- FastClick -->
<script src="//cdnjs.cloudflare.com/ajax/libs/fastclick/1.0.6/fastclick.min.js"></script>
<!-- NProgress -->
<script src="//cdnjs.cloudflare.com/ajax/libs/nprogress/0.2.0/nprogress.js"></script>
<script src="{{ asset('js/datepicker/daterangepicker.js') }}"></script>


<!-- sweet alert -->
<script src="//cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>

<!-- Moment -->
<script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.19.1/moment.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.19.1/locale/fr.js"></script>
<!-- jquery mask-->
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.10/jquery.mask.min.js"></script>
<!-- jquery dataTables-->
<script src="//cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
<!-- dataTables.bootstrap.-->
<script src="//cdn.datatables.net/1.10.15/js/dataTables.bootstrap.min.js"></script>

<!-- dataTables.bootstrap.-->
<script src="//cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.0/Chart.min.js"></script>


<!-- combodate.js-->
<script src="{{ asset('js/combodate.js') }}"></script>


<!-- dropzone.js-->
<script src="{{ asset('js/dropzone.js') }}"></script>

<!-- dropzone.css-->
<link src="{{ asset('css/dropzone.css') }}"></link>



<!-- TimePicker-->

<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.47/js/bootstrap-datetimepicker.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.47/css/bootstrap-datetimepicker.min.css">



<!-- handsontable -->

<script src="https://cdnjs.cloudflare.com/ajax/libs/handsontable/0.34.4/handsontable.full.min.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/handsontable/0.34.4/handsontable.full.min.css">



{% block modals %}{% endblock modals %}




<script src="{{ asset('js/notify.min.js') }}"></script>
<script src="{{ asset('js/app.js') }}"></script>
<script src="{{ asset('js/conso.js') }}"></script>


</body>
</html>
