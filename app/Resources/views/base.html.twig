<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}ACHAT-CENTRALE CRM{% endblock %}</title>
    <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>


    {% block stylesheets %}
        <!-- Bootstrap -->
        <link href="{{ asset('js/vendors/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- Font Awesome -->
        <link href="{{ asset('js/vendors/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
        <!-- Semantic ui -->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.10/semantic.css" rel="stylesheet">
        <!-- sweet alert-->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css" rel="stylesheet">
        <!-- jquery ui-->
        <link href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.css" rel="stylesheet">
        <!-- datatables bootstrap-->
        <link href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css" rel="stylesheet">

        <!-- Material float button -->
        <link href="{{ asset('css/mfb.min.css') }}" rel="stylesheet">
    {% endblock stylesheets %}

    <!-- Custom Theme Style -->
    <link href="{{ asset('assets/css/custom.min.css') }}" rel="stylesheet">
    <link href="{{ asset('css/style.css') }}" rel="stylesheet">

</head>

<body class="nav-md">
<div class="container body">
    <div class="main_container">


        {% include 'SiteBundle:Layout:Sidebar.html.twig' %}
        {% include 'SiteBundle:Layout:topnav.html.twig' %}


        <!-- page content -->
        <div class="right_col" role="main">
            <div class="container-content-page">
                {% block top_content_header %}
                    {#<div class="page-title">
                        <div class="title_left">
                            <h3>{% block page_title %}</h3>Page Title{% endblock page_title %}
                        </div>


                    </div>#}
                {% endblock top_content_header %}

                <div class="clearfix"></div>


                {% block body %}








                {% endblock %}


            </div>
        </div>
        <!-- /page content -->




        {% include 'SiteBundle:Layout:footer.html.twig' %}


    </div>
</div>

    <!-- semantic ui -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.10/semantic.min.js"></script>
     <!-- jQuery ui -->
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"></script>

    {#boostrap validator#}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/1000hz-bootstrap-validator/0.11.9/validator.js"></script>
    <!-- Bootstrap -->
    <script src="{{ asset('js/vendors/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    <!-- FastClick -->
    <script src="{{ asset('js/vendors/fastclick/lib/fastclick.js') }}"></script>
    <!-- NProgress -->
    <script src="{{ asset('js/vendors/nprogress/nprogress.js') }}"></script>
    <script src="{{ asset('assets/js/datepicker/daterangepicker.js') }}"></script>

    <!-- Material float button -->
    <script src="{{ asset('js/mfb.min.js') }}"></script>

    <!-- sweet alert -->
    <script src="//cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>

 <!-- jquery mask-->
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.10/jquery.mask.min.js"></script>
<!-- jquery dataTables-->
    <script src="https://cdn.datatables.net/1.10.15/js/jquery.dataTables.min.js"></script>
<!-- dataTables.bootstrap.-->
    <script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap.min.js"></script>

{% block modals %}{% endblock modals %}

<!-- Custom Theme Scripts -->
<script src="{{ asset('js/app.js') }}"></script>


</body>
</html>
