<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SiteBundle\Entity\Familles" table="FAMILLES">
    <indexes>
      <index name="IDX_59A7616B90B34CA6" columns="RA_ID"/>
    </indexes>
    <id name="faId" type="integer" column="FA_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="faNom" type="string" column="FA_NOM" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="faDescr" type="text" column="FA_DESCR" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="faTri" type="integer" column="FA_TRI" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="faTempo" type="string" column="FA_TEMPO" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="insDate" type="datetime" column="INS_DATE" nullable="true"/>
    <field name="insUser" type="string" column="INS_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="majDate" type="datetime" column="MAJ_DATE" nullable="true"/>
    <field name="majUser" type="string" column="MAJ_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="ra" target-entity="Rayons" fetch="LAZY">
      <join-columns>
        <join-column name="RA_ID" referenced-column-name="RA_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
