<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SiteBundle\Entity\Rayons" table="RAYONS">
    <id name="raId" type="integer" column="RA_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="raNom" type="string" column="RA_NOM" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="raDescr" type="text" column="RA_DESCR" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="raPicto" type="string" column="RA_PICTO" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="raTri" type="integer" column="RA_TRI" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="raTempo" type="string" column="RA_TEMPO" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="insDate" type="datetime" column="INS_DATE" nullable="true"/>
    <field name="insUser" type="string" column="INS_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="majDate" type="datetime" column="MAJ_DATE" nullable="true"/>
    <field name="majUser" type="string" column="MAJ_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
