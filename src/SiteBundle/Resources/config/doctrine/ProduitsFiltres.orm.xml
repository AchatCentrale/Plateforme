<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SiteBundle\Entity\ProduitsFiltres" table="PRODUITS_FILTRES">
    <indexes>
      <index name="IDX_D0ABA9BFA8DFE7B7" columns="PR_ID"/>
      <index name="IDX_D0ABA9BFC067550B" columns="FI_ID"/>
    </indexes>
    <id name="pfId" type="integer" column="PF_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="insDate" type="datetime" column="INS_DATE" nullable="true"/>
    <field name="insUser" type="string" column="INS_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="pr" target-entity="Produits" fetch="LAZY">
      <join-columns>
        <join-column name="PR_ID" referenced-column-name="PR_ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fi" target-entity="Filtres" fetch="LAZY">
      <join-columns>
        <join-column name="FI_ID" referenced-column-name="FI_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
