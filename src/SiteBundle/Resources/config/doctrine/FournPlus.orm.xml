<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SiteBundle\Entity\FournPlus" table="FOURN_PLUS">
    <id name="fo" association-key="true"/>
    <field name="fpPdfTitre1" type="string" column="FP_PDF_TITRE1" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fpPdf1" type="string" column="FP_PDF1" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fpPdfTitre2" type="string" column="FP_PDF_TITRE2" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fpPdf2" type="string" column="FP_PDF2" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fpPlus" type="string" column="FP_PLUS" length="2000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fpConseils" type="string" column="FP_CONSEILS" length="2000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fpTempo" type="string" column="FP_TEMPO" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="insDate" type="datetime" column="INS_DATE" nullable="true"/>
    <field name="insUser" type="string" column="INS_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="majDate" type="datetime" column="MAJ_DATE" nullable="true"/>
    <field name="majUser" type="string" column="MAJ_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-one field="fo" target-entity="Fournisseurs" fetch="LAZY">
      <join-columns>
        <join-column name="FO_ID" referenced-column-name="FO_ID"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
