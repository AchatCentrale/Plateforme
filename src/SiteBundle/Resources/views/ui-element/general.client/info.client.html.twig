<div class="hidden-centrale">
    <p id="centrale">{{ centrale }}</p>
    <p id="id">{{ client[0]["CL_ID"] }}</p>
</div>

{% set randomPassword = [] %}
    {% for i in 1..5 %}
        {% set alpha = 'abcdefghijklmnopqrstuvwxyz' %}
        {% set alphaUpper = alpha|upper %}
        {% set numbers = '0123456789' %}
        {% set randomCharacter = random(alpha ~ alphaUpper ~ numbers) %}
        {% set randomPassword = randomPassword|merge([randomCharacter]) %}
    {% endfor %}
{% set randomPassword = randomPassword|join %}


<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>FICHE CLIENT</h2>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <div>
                    <div class="ui equal width grid raison-soc">
                        <div class="row">
                            <div class="seven wide column">
                                <div class="ui equal width grid container-hastag-label">
                                    <div class="column">
                                        <h3 class="raison-soc-titre">
                                            <i class="building icon"></i> {{ client[0]["CL_RAISONSOC"] |  convert_encoding('UTF-8', 'Windows-1252') | upper }}
                                            {% if client[0]["CL_STATUS"] == 2 %}
                                                <p>Bloqué</p>
                                            {% endif %}
                                        </h3>
                                    </div>
                                    <div class="column">
                                        {{ centrale  | centraleLabel | raw }}
                                    </div>
                                </div>
                            </div>
                            <div class="three wide column">
                                <h3><span class="pastille-ref green"></span> {{ client[0]["CL_REF"] }}</h3>
                            </div>
                            <div class="column two wide">
                                <a class="ui label">
                                    <i class="user icon"></i>
                                    {{ client[0]["INS_USER"] }}
                                </a>
                            </div>
                            <div class="column">
                                <h3> Crée
                                    le {{ client[0]["INS_DATE"] | localizeddate('full', 'none', null, null, 'd MMMM Y') }}</h3>
                            </div>
                        </div>


                    </div>


                    <div class="container_detail_client">

                        <!-- Nav tabs -->
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active"><a href="#last_activity"
                                                                      aria-controls="last_activity" role="tab"
                                                                      data-toggle="tab">Dernières activités</a></li>
                            <li role="presentation"><a href="#general" aria-controls="general" role="tab"
                                                       data-toggle="tab">Géneral</a></li>
                            <li role="presentation"><a href="#utilisateur" aria-controls="utilisateur" role="tab"
                                                       data-toggle="tab">Utilisateurs</a></li>
                            <li role="presentation"><a href="#detail" aria-controls="detail" role="tab"
                                                       data-toggle="tab">Détails</a></li>
                            <li role="presentation"><a href="#conso" aria-controls="conso" role="tab" data-toggle="tab">Consommation</a>
                            </li>
                            <li role="presentation"><a href="#admin" aria-controls="admin" role="tab" data-toggle="tab">Admninistration </a>
                            </li>
                        </ul>

                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="last_activity">
                                <div class="container_last_activity">
                                    <div class="last_notes">
                                        <h2>Dernières notes</h2>
                                        <div class="notes_listes">
                                            <div class="container_ajout_note">
                                                <div class="input-group">

                                                    <input type="text" class="form-control" id="add-note"
                                                           placeholder="Consigner une note pour le client {{ client[0]["CL_RAISONSOC"] |  convert_encoding('UTF-8', 'Windows-1252') }}">
                                                    <span class="input-group-btn">
                                                        <button class="btn btn-secondar btn-success add-note"
                                                                type="button">Enregistrer</button>
                                                        <i class="window close icon empty_notes"></i>
                                                    </span>
                                                </div>
                                            </div>

                                            <div class="notes">
                                                {% if note %}
                                                    <ul>
                                                        {% for n in note %}
                                                            <li>

                                                                <div class="ui grid">
                                                                    <div class="fifteen wide column">
                                                                        <p>
                                                                            <span class="bold">{{ n["INS_DATE"] | localizeddate('full', 'none', null, null, 'd MMMM Y') }}</span>: {{ n["CN_NOTE"] }}
                                                                        </p>
                                                                    </div>
                                                                    <div class="one wide column">
                                                                        <div class="action_delete">
                                                                            <i class="close icon remove_note"
                                                                               data-note="{{ n["CN_ID"] }}"></i>
                                                                        </div>
                                                                    </div>
                                                                </div>

                                                            </li>
                                                            <div class="horizontal_line"></div>
                                                        {% endfor %}
                                                    </ul>
                                                {% else %}
                                                    <p>Il n'y aucune note pour le client</p>
                                                {% endif %}
                                            </div>

                                        </div>
                                    </div>

                                    <div class="last_notes">
                                        <h2>Dernières actions</h2>
                                        <div class="notes_listes">

                                            {% if task %}
                                                <div class="">
                                                    <ul class="to_do">
                                                        {% for tasks in task %}
                                                            <li data-id="{{ tasks['CLA_ID'] }}"
                                                                class="cursor detail-tache-home-client">
                                                                <p>{{ tasks['CLA_ECHEANCE'] | localizeddate('full', 'none', null, null, 'd MMMM Y') }}
                                                                    - {{ tasks['CLA_NOM'] | raw |  convert_encoding('UTF-8', 'Windows-1252') }}
                                                                    - {{ tasks['CLA_DESCR'] | raw |  convert_encoding('UTF-8', 'Windows-1252') }} {{ tasks['CLA_STATUS'] | etat | raw }}</p>
                                                            </li>
                                                        {% endfor %}
                                                    </ul>
                                                </div>

                                            {% else %}
                                                <h5>Pas de taches pour le moment</h5>
                                            {% endif %}

                                        </div>
                                    </div>

                                </div>
                            </div>
                            <div role="tabpanel" class="tab-pane" id="general">
                                <div id="container_detail_general">

                                    <div class="ui grid">
                                        <div class="two wide column ">
                                            <div class="profile_img">
                                                <div id="crop-avatar">
                                                    {% if client[0]["CL_LOGO"] %}
                                                        <div class="container-hover-image">
                                                            <img class="img-responsive img-rounded  avatar-view"
                                                                 src="{{ centrale | centraleUrl }}/UploadFichiers/Uploads/CLIENT_{{ client[0]["CL_ID"] }}/{{ client[0]["CL_LOGO"] }}"
                                                                 alt="Logo" style="width: 90%">
                                                        </div>
                                                    {% endif %}
                                                </div>
                                            </div>
                                        </div>
                                        <div class="four wide column">
                                            <ul class="list-unstyled user_data">
                                                <li><i class="fa fa-map-marker user-profile-icon"></i>
                                                    {{ client[0]["CL_ADRESSE1"] | formatVille | convert_encoding('UTF-8', 'Windows-1252') }}
                                                    <br>
                                                    {{ client[0]["CL_CP"] }} {{ client[0]["CL_VILLE"] | formatVille | convert_encoding('UTF-8', 'Windows-1252') }}
                                                </li>
                                            </ul>
                                        </div>
                                        <div class="ten wide column" style="padding-top: 35px;">
                                            <div class="ui equal width grid">
                                                <div class="row" style="padding-left: 23px;">
                                                    <div class="nine wide column">
                                                        <a href="mailto:{{ client[0]["CL_MAIL"] }}"><p><span
                                                                        class="ui header">Adresse mail</span>
                                                                &nbsp; {{ client[0]["CL_MAIL"] }}</p></a>
                                                    </div>
                                                    <div class="seven wide column">
                                                        <a href="mailto:{{ client[0]["CL_MAIL"] }}"><p><span
                                                                        class="ui header">Téléphone</span>
                                                                &nbsp; {{ client[0]["CL_TEL"] | phone }}</p></a>
                                                    </div>
                                                    <div class="column">
                                                        {% if client[0]["CL_WEB"] %}
                                                            <li class="m-top-xs web-li">
                                                                <a href="{{ relative_path(client[0]["CL_WEB"] ) }}">{{ client[0]["CL_WEB"] }}</a>
                                                            </li>
                                                        {% else %}
                                                            <p>Pas de site web</p>
                                                        {% endif %}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="ui grid">
                                        <div class="five wide column">
                                            <div class="info-client-detail">
                                                <p class="title-detail-client ">Siret</p>
                                                <p class="cursor label-background" data-index="siret">308 381 086
                                                    00000</p>
                                            </div>
                                        </div>
                                        <div class="five wide column">
                                            <div class="info-client-detail">
                                                <p class="title-detail-client ">Éffectif</p>
                                                <p class="cursor label-background" data-index="effectif">0</p>
                                            </div>
                                        </div>
                                        <div class="six wide column">
                                            <div class="info-client-detail">
                                                <p class="title-detail-client ">Chiffre d'affaire</p>
                                                <p class="cursor label-background" data-index="ca">0</p>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="ui grid">
                                        <div class="five wide column">
                                            <div class="info-client-detail">
                                                <p class="title-detail-client ">Activité</p>
                                                {% if  activite == "Aucune activité pour le moment" %}
                                                    <p class="cursor label-background"
                                                       data-index="activite">{{ activite }}</p>

                                                {% else %}
                                                    {% if activite %}
                                                        <p class="cursor label-background"
                                                           data-index="activite">{{ activite[0]["AC_DESCR"] |  convert_encoding('UTF-8', 'Windows-1252') }}</p>
                                                    {% else %}
                                                        <p class="cursor label-background"
                                                           data-index="activite">Pas d'activité enregistrer encore</p>
                                                    {% endif %}
                                                {% endif %}
                                            </div>
                                        </div>
                                        <div class="five wide column">
                                            <div class="info-client-detail">
                                                <p class="title-detail-client ">Groupe</p>

                                                {% if groupe %}
                                                    <p class="ui label large green">{{ groupe[0]["GR_DESCR"] |  convert_encoding('UTF-8', 'Windows-1252') }}</p>
                                                    <br>
                                                {% else %}
                                                    <p>Aucun groupe renseigné</p>
                                                {% endif %}

                                            </div>
                                        </div>
                                        <div class="five wide column">
                                            <div class="info-client-detail">
                                                <p class="title-detail-client ">Chiffre d'affaire</p>
                                                <p class="cursor label-background" data-index="ca">0</p>
                                            </div>
                                        </div>
                                    </div>


                                </div>
                            </div>
                            <div role="tabpanel" class="tab-pane" id="detail">...</div>
                            <div role="tabpanel" class="tab-pane" id="utilisateur">
                                <div class="container_liste_users">
                                    <table class="ui celled table">
                                        <thead>
                                        <tr>
                                            <th>Noms du contact</th>
                                            <th>Poste</th>
                                            <th>Mail</th>
                                            <th>Tel</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        {% for u in user %}
                                            {% if u["CC_STATUS"] is same as(0) %}
                                                <tr>
                                                    <td data-label="Noms du contact"><a
                                                                href="//secure.{{ centrale_link }}.fr/Login.asp?LOGIN={{ u["CC_MAIL"] }}&PASS={{ u["CC_PASS"] }}"><i
                                                                    class="user icon"></i>
                                                            {{ u["CC_PRENOM"] |  convert_encoding('UTF-8', 'Windows-1252') }} {{ u["CC_NOM"] |  convert_encoding('UTF-8', 'Windows-1252') }}
                                                        </a></td>
                                                    <td data-label="Poste">
                                                        <b>{{ u["CC_FONCTION"] | convert_encoding('UTF-8', 'Windows-1252') }}</b>
                                                    </td>
                                                    <td data-label="Mail"><a
                                                                href="mailto:{{ u["CC_MAIL"] }}">{{ u["CC_MAIL"] }}</a>
                                                    </td>
                                                    <td data-label="Tel">{{ u["CC_TEL"]  | phone }}</td>
                                                </tr>
                                            {% endif %}
                                        {% endfor %}

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                            <div role="tabpanel" class="tab-pane" id="conso">
                                <div class="container_conso_client">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <h3>Depuis le {{ client[0]["INS_DATE"] | localizeddate('full', 'none', null, null, 'd MMMM Y') }}, il y a eu {{ topEconomie | localizedcurrency('EUR') }} d'économies</h3>
                                        </div>
                                        <div class="col-md-4">
                                            <h3>Top fournisseurs</h3>
                                            <br>
                                            <table class="ui very basic collapsing celled table">
                                                <thead>
                                                <tr>
                                                    <th><h4>Fournisseur</h4></th>
                                                    <th><h4>Economie</h4></th>
                                                </tr>
                                                </thead>
                                                <tbody>
                                                {% for t in topConso %}
                                                    <tr>
                                                        <td>
                                                            {{ t["FO_ID"] | capitalize }}
                                                        </td>
                                                        <td>
                                                            {{ t["Economie"] | localizedcurrency('EUR') }}

                                                        </td>
                                                    </tr>
                                                {% endfor %}
                                                </tbody>
                                            </table>
                                        </div>
                                        <div class="col-md-4">
                                            <h3>Dernières consommations</h3>
                                            <br>
                                            <table class="ui very basic collapsing celled table">
                                                <thead>
                                                <tr>
                                                    <th><h4>Fournisseurs</h4></th>
                                                    <th><h4>Date de conso</h4></th>
                                                    <th><h4>Economie</h4></th>
                                                </tr>
                                                </thead>
                                                <tbody>
                                                {% for l in lastConso %}
                                                    <tr>
                                                        <td>{{ l["FO_ID"] | capitalize }}</td>
                                                        <td>{{ l["CLC_DATE"]   }}</td>
                                                        <td>{{ l["Economie"] | localizedcurrency('EUR') }}</td>
                                                    </tr>
                                                {% endfor %}

                                                </tbody>
                                            </table>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div role="tabpanel" class="tab-pane" id="admin">
                                <button id="modifierProfil" type="button" class="btn btn-success" data-toggle="modal"
                                        data-target="#myModal">
                                    <i class="fa fa-edit edit-client m-right-xs"></i>Modifier le profil
                                </button>
                                <div class="ajoutuser">
                                    <button class="ui button client-user-new" type="button" data-toggle="modal"
                                            data-target="#client-user-new">
                                        Ajouter un utilisateur
                                    </button>
                                </div>
                            </div>
                        </div>

                    </div>

