<div class="ui three column divided grid">

    <div class="row">
        {% for fe in feed.feed %}

            <div class="column">
                <div class="ui  feed">
                    {% if fe | getClass == "MessageEntete" %}

                        <div class="event">
                            <div>
                                <img class="ui tiny circular image avatar-feed"
                                     src="{{ asset('assets/images/icon/logo_t.png') }}">

                            </div>
                            <div class="content">
                                <div class="summary">
                                    Tickets
                                    <div class="date">
                                        {{ fe.insDate |Time }}
                                    </div>
                                </div>
                                <div class="extra text">
                                    {{ fe.meSujet }}

                                </div>
                                <div class="meta">

                                </div>
                            </div>
                        </div>

                    {% endif %}

                    {% if fe |getClass == "ClientsNotes" %}
                        <div class="event">
                            <div>
                                <img class="ui tiny circular image avatar-feed"
                                     src="{{ asset('assets/images/icon/logo_n.png') }}">

                            </div>
                            <div class="content">
                                <div class="summary">
                                    Notes
                                    <div class="date">
                                        {{ fe.insDate |Time }}
                                    </div>
                                </div>
                                <div class="extra text">
                                    {{ fe.cnNote }}

                                </div>
                                <div class="meta">

                                </div>
                            </div>
                        </div>

                    {% endif %}

                    {% if fe |getClass == "ClientsTaches" %}
                        <div class="event">
                            <div>
                                <img class="ui tiny circular image avatar-feed"
                                     src="{{ asset('assets/images/icon/logo_n.png.png') }}">

                            </div>
                            <div class="content">
                                <div class="summary">
                                    You submitted a new post to the page
                                    <div class="date">
                                        {{ fe.insDate |Time }}
                                    </div>
                                </div>
                                <div class="extra text">
                                    I'm having a BBQ this weekend. Come by around 4pm if you can.
                                </div>
                                <div class="meta">

                                </div>
                            </div>
                        </div>

                    {% endif %}


                </div>
            </div>


        {% endfor %}

    </div>


</div>

