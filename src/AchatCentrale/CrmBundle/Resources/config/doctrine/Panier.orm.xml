<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\Panier" table="PANIER">
    <indexes>
      <index name="IDX_D434CA743F592A49" columns="CL_ID"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="meId" type="integer" column="ME_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idCommande" type="integer" column="ID_COMMANDE" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="prId" type="integer" column="PR_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quantite" type="integer" column="QUANTITE" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="prixUht" type="float" column="PRIX_UHT" precision="53" scale="0" nullable="true"/>
    <field name="insDate" type="datetime" column="INS_DATE" nullable="true"/>
    <field name="insUser" type="string" column="INS_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="majDate" type="datetime" column="MAJ_DATE" nullable="true"/>
    <field name="majUser" type="string" column="MAJ_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="cl" target-entity="Clients" fetch="LAZY">
      <join-columns>
        <join-column name="CL_ID" referenced-column-name="CL_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
