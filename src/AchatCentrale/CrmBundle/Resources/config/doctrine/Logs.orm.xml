<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\Logs" table="LOGS">
    <id name="loId" type="integer" column="LO_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="soId" type="integer" column="SO_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="loIdent" type="string" column="LO_IDENT" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="loIdentNum" type="integer" column="LO_IDENT_NUM" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="loCode" type="string" column="LO_CODE" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="loDate" type="datetime" column="LO_DATE" nullable="true"/>
    <field name="loDescr" type="string" column="LO_DESCR" length="5000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="insDate" type="datetime" column="INS_DATE" nullable="true"/>
    <field name="insUser" type="string" column="INS_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
