<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\NousContacter" table="NOUS_CONTACTER">
    <id name="ncId" type="integer" column="NC_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="soId" type="integer" column="SO_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ncSociete" type="string" column="NC_SOCIETE" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncNom" type="string" column="NC_NOM" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncPrenom" type="string" column="NC_PRENOM" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncSiret" type="string" column="NC_SIRET" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncMail" type="string" column="NC_MAIL" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncTel" type="string" column="NC_TEL" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncObjet" type="string" column="NC_OBJET" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncCommentaire" type="string" column="NC_COMMENTAIRE" length="7000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncTempo" type="string" column="NC_TEMPO" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ncDate" type="datetime" column="NC_DATE" nullable="true"/>
  </entity>
</doctrine-mapping>
