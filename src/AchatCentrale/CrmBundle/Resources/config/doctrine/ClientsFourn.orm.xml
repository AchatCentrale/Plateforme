<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\ClientsFourn" table="CLIENTS_FOURN">
    <indexes>
      <index name="IDX_A04F9C213F592A49" columns="CL_ID"/>
    </indexes>
    <id name="cfId" type="integer" column="CF_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="foId" type="integer" column="FO_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cfUser" type="string" column="CF_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cfPass" type="string" column="CF_PASS" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="cl" target-entity="Clients" fetch="LAZY">
      <join-columns>
        <join-column name="CL_ID" referenced-column-name="CL_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
