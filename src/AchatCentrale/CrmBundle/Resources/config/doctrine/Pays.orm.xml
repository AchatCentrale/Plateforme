<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\Pays" table="PAYS">
    <id name="paId" type="integer" column="PA_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="paCode" type="string" column="PA_CODE" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="paDescr" type="string" column="PA_DESCR" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="paProv" type="string" column="PA_PROV" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
