<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\Tarifs" table="TARIFS">
    <id name="taId" type="integer" column="TA_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="soId" type="integer" column="SO_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="taEffectif" type="string" column="TA_EFFECTIF" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="taUnite" type="integer" column="TA_UNITE" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="taPrix" type="float" column="TA_PRIX" precision="53" scale="0" nullable="true"/>
    <field name="taPrixRemise" type="float" column="TA_PRIX_REMISE" precision="53" scale="0" nullable="true"/>
    <field name="taDate" type="datetime" column="TA_DATE" nullable="true"/>
  </entity>
</doctrine-mapping>
