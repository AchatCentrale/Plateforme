<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\MessageDetail" table="MESSAGE_DETAIL">
    <indexes>
      <index name="IDX_3DB4A13DFD61DBA2" columns="ME_ID"/>
    </indexes>
    <id name="mdId" type="integer" column="MD_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ccId" type="integer" column="CC_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fcId" type="integer" column="FC_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="usId" type="integer" column="US_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mdDate" type="datetime" column="MD_DATE" nullable="true"/>
    <field name="mdCorps" type="text" column="MD_CORPS" length="16" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mdTempo" type="string" column="MD_TEMPO" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="insDate" type="datetime" column="INS_DATE" nullable="true"/>
    <field name="insUser" type="string" column="INS_USER" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="me" target-entity="MessageEntete" fetch="LAZY">
      <join-columns>
        <join-column name="ME_ID" referenced-column-name="ME_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
