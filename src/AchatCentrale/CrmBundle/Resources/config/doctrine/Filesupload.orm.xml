<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\Filesupload" table="FilesUpload">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fichier" type="string" column="Fichier" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="taille" type="integer" column="Taille" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="taillex" type="integer" column="TailleX" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="tailley" type="integer" column="TailleY" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="commentaires" type="string" column="Commentaires" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="docid" type="integer" column="DocID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateupload" type="datetime" column="DateUpload" nullable="true"/>
  </entity>
</doctrine-mapping>
