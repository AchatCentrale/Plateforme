<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\Historique" table="historique">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mailExp" type="string" column="mail_exp" length="255" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="mailDest" type="string" column="mail_dest" length="255" nullable="false">
      <options>
        <option name="fixed">1</option>
      </options>
    </field>
    <field name="vdate" type="datetime" column="vdate" nullable="false"/>
    <field name="codeRetour" type="string" column="code_retour" length="3000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idCodeRetour" type="integer" column="id_code_retour" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="codeErreur" type="string" column="code_erreur" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subjectmessage" type="string" column="SubjectMessage" length="500" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bodymessage" type="string" column="BodyMessage" length="5000" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
