<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AchatCentrale\CrmBundle\Entity\MessageFichiers" table="MESSAGE_FICHIERS">
    <indexes>
      <index name="IDX_6998CDC7FD61DBA2" columns="ME_ID"/>
    </indexes>
    <id name="mfId" type="integer" column="MF_ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mdId" type="integer" column="MD_ID" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="mfFichier" type="string" column="MF_FICHIER" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mfTaille" type="float" column="MF_TAILLE" precision="53" scale="0" nullable="true"/>
    <field name="mfDate" type="datetime" column="MF_DATE" nullable="true"/>
    <field name="mfTempo" type="string" column="MF_TEMPO" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="me" target-entity="MessageEntete" fetch="LAZY">
      <join-columns>
        <join-column name="ME_ID" referenced-column-name="ME_ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
